<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="100dp"
        android:height="100dp">

        <shape
            android:shape="oval">
            <solid android:color="#ffffff"></solid>
            <size android:width="40dp" android:height="40dp"></size>
        </shape>

    </item>

    <item
        android:height="20dp"
        android:width="10dp"
        android:top="40dp"
        android:left="36dp">

        <shape
            android:shape="rectangle">
            <corners android:radius="5dp"></corners>
            <solid android:color="#B2B2B2"></solid>
            <size android:height="29dp"
                android:width="15dp"></size>


        </shape>
    </item>
    <item
        android:height="20dp"
        android:width="10dp"
        android:top="40dp"
        android:left="50dp">

        <shape
            android:shape="rectangle">
            <corners android:radius="5dp"></corners>
            <solid android:color="#B2B2B2"></solid>
            <size android:height="10dp"
                android:width="10dp"></size>


        </shape>
    </item>



</layer-list>